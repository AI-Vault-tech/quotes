    "authorId": "doug-coupland",
    "tags": [
      "birds",
      "miracle",
      "simplicity"
    ],
    "themes": [
      "nature",
      "wildlife"
    ],
    "schools": [
      "literature"
    ],
    "id": "nature-45",
    "createdAt": "2025-11-29",
    "updatedAt": "2025-11-29"
  },
  {
