    "authorId": "ralph-waldo-emerson",
    "tags": [
      "sky",
      "nourishment",
      "perception"
    ],
    "themes": [
      "nature",
      "skies"
    ],
    "schools": [
      "transcendentalism"
    ],
    "id": "nature-33",
    "createdAt": "2025-11-29",
    "updatedAt": "2025-11-29"
  },
  {
    "text": "Moonlight is sculpture.",
    "author": "Nathaniel Hawthorne",
    "authorId": "nathaniel-hawthorne",
