      "earth",
      "flowers",
      "joy"
    ],
    "themes": [
      "nature",
      "flowers"
    ],
    "schools": [
      "transcendentalism"
    ],
    "id": "nature-46",
    "createdAt": "2025-11-29",
    "updatedAt": "2025-11-29"
  },
