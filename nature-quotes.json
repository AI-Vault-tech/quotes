    "authorId": "helen-keller",
    "tags": [
      "optimism",
      "positivity",
      "light"
    ],
    "themes": [
      "nature",
      "skies"
    ],
    "schools": [
      "inspiration"
    ],
    "id": "nature-31",
    "createdAt": "2025-11-29",
    "updatedAt": "2025-11-29"
  },
  {
    "text": "The sun does not shine for a few trees and flowers, but for the wide world's joy.",
