    "authorId": "kahlil-gibran",
    "tags": [
      "poetry",
      "trees",
      "expression"
    ],
    "themes": [
      "nature",
      "forests"
    ],
    "schools": [
      "literature"
    ],
    "id": "nature-15",
    "createdAt": "2025-11-29",
    "updatedAt": "2025-11-29"
  },
  {
