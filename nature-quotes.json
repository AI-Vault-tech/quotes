    "authorId": "david-mccullough-jr",
    "tags": [
      "challenge",
      "enjoyment",
      "perspective"
    ],
    "themes": [
      "nature",
      "mountains"
    ],
    "schools": [
      "history"
    ],
    "id": "nature-28",
    "createdAt": "2025-11-29",
    "updatedAt": "2025-11-29"
  },
  {
    "text": "Mountains are the cathedrals where I practice my religion.",
